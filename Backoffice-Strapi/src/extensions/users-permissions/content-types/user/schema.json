{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "firstname": {
      "type": "string",
      "minLength": 3,
      "maxLength": 50
    },
    "lastname": {
      "type": "string",
      "minLength": 3,
      "maxLength": 50
    },
    "point": {
      "type": "integer",
      "minimum": 0,
      "maximum": 25
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "phone": {
      "type": "string"
    },
    "institution": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::institution.institution"
    },
    "newsletter": {
      "type": "boolean"
    },
    "rewards": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::reward.reward",
      "mappedBy": "user"
    },
    "State": {
      "type": "boolean"
    },
    "inscriptionDate": {
      "type": "datetime"
    },
    "participation": {
      "type": "boolean"
    },
    "participationDate": {
      "type": "datetime"
    }
  }
}